{
  "id": null,
  "uid": "advanced-task-queue",
  "title": "üöÄ Advanced Task Queue Dashboard - Production Analytics",
  "tags": ["task-queue", "production", "monitoring", "analytics"],
  "style": "dark",
  "timezone": "browser",
  "refresh": "5s",
  "schemaVersion": 27,
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
  },
  "templating": {
    "list": [
      {
        "name": "task_type",
        "type": "query",
        "label": "Task Type",
        "query": "label_values(consumer_tasks_processed_total, task_type)",
        "refresh": 1,
        "includeAll": true,
        "allValue": ".*",
        "multi": true,
        "current": {"selected": true, "text": "All", "value": "$__all"},
        "options": [],
        "datasource": null
      },
      {
        "name": "queue_name",
        "type": "query", 
        "label": "Queue Name",
        "query": "label_values(consumer_tasks_processed_total, queue_name)",
        "refresh": 1,
        "includeAll": true,
        "allValue": ".*",
        "multi": true,
        "current": {"selected": true, "text": "All", "value": "$__all"},
        "options": [],
        "datasource": null
      },
      {
        "name": "interval",
        "type": "interval",
        "label": "Interval",
        "query": "1m,5m,10m,30m,1h",
        "current": {"selected": true, "text": "5m", "value": "5m"},
        "auto": true,
        "auto_count": 30,
        "auto_min": "10s"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "üéØ System Overview - Real-time Status",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(increase(producer_tasks_sent_total{task_type=~\"$task_type\", queue_name=~\"$queue_name\"}[$interval]))",
          "legendFormat": "Tasks Sent",
          "refId": "A"
        },
        {
          "expr": "sum(increase(consumer_tasks_processed_total{status=\"success\", task_type=~\"$task_type\", queue_name=~\"$queue_name\"}[$interval]))",
          "legendFormat": "Successfully Processed",
          "refId": "B"
        },
        {
          "expr": "sum(increase(consumer_tasks_processed_total{status=\"failed\", task_type=~\"$task_type\", queue_name=~\"$queue_name\"}[$interval]))",
          "legendFormat": "Failed Tasks",
          "refId": "C"
        },
        {
          "expr": "sum(consumer_active_tasks{task_type=~\"$task_type\", queue_name=~\"$queue_name\"})",
          "legendFormat": "Currently Processing",
          "refId": "D"
        },
        {
          "expr": "sum(increase(consumer_task_retries_total{task_type=~\"$task_type\"}[$interval]))",
          "legendFormat": "Retries",
          "refId": "E"
        },
        {
          "expr": "sum(increase(consumer_dead_letter_queue_total{task_type=~\"$task_type\"}[$interval]))",
          "legendFormat": "DLQ Messages",
          "refId": "F"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 10},
              {"color": "orange", "value": 50},
              {"color": "red", "value": 100}
            ]
          },
          "unit": "short",
          "decimals": 0,
          "noValue": "0"
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "Failed Tasks"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}
            ]
          },
          {
            "matcher": {"id": "byName", "options": "DLQ Messages"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "dark-red"}}
            ]
          },
          {
            "matcher": {"id": "byName", "options": "Successfully Processed"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}
            ]
          }
        ]
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "title": "üìä Task Flow Analytics - Performance Tracking",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(producer_tasks_sent_total{task_type=~\"$task_type\", queue_name=~\"$queue_name\"}[1m]) * 60",
          "legendFormat": "üì§ Sent: {{task_type}} ‚Üí {{queue_name}}",
          "refId": "A"
        },
        {
          "expr": "rate(consumer_tasks_processed_total{status=\"success\", task_type=~\"$task_type\", queue_name=~\"$queue_name\"}[1m]) * 60",
          "legendFormat": "‚úÖ Success: {{task_type}}",
          "refId": "B"
        },
        {
          "expr": "rate(consumer_tasks_processed_total{status=\"failed\", task_type=~\"$task_type\", queue_name=~\"$queue_name\"}[1m]) * 60",
          "legendFormat": "‚ùå Failed: {{task_type}}",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 15,
            "gradientMode": "hue",
            "showPoints": "never",
            "pointSize": 5,
            "stacking": {"mode": "none"},
            "axisPlacement": "auto",
            "axisLabel": "Tasks per minute",
            "scaleDistribution": {"type": "linear"},
            "hideFrom": {"legend": false, "tooltip": false, "vis": false},
            "thresholdsStyle": {"mode": "off"}
          },
          "color": {"mode": "palette-classic"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "red", "value": 80}
            ]
          },
          "unit": "reqps",
          "min": 0
        },
        "overrides": [
          {
            "matcher": {"id": "byRegexp", "options": "/‚ùå Failed.*/"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}},
              {"id": "custom.fillOpacity", "value": 30}
            ]
          }
        ]
      },
      "options": {
        "tooltip": {"mode": "multi", "sort": "desc"},
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": ["lastNotNull", "max", "mean"],
          "values": ["value", "percent"]
        }
      },
      "gridPos": {"h": 10, "w": 24, "x": 0, "y": 8}
    },
    {
      "id": 3,
      "title": "üö® Error & Retry Analysis",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(consumer_task_errors_total{task_type=~\"$task_type\"}[1m]) * 60",
          "legendFormat": "üî• Errors: {{task_type}} | {{error_type}}",
          "refId": "A"
        },
        {
          "expr": "rate(consumer_task_retries_total{task_type=~\"$task_type\"}[1m]) * 60",
          "legendFormat": "üîÑ Retries: {{task_type}}",
          "refId": "B"
        },
        {
          "expr": "rate(consumer_dead_letter_queue_total{task_type=~\"$task_type\"}[1m]) * 60",
          "legendFormat": "üíÄ DLQ: {{task_type}} | {{reason}}",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 25,
            "showPoints": "auto",
            "pointSize": 4,
            "stacking": {"mode": "none"}
          },
          "color": {"mode": "palette-classic"},
          "unit": "reqps",
          "min": 0
        },
        "overrides": [
          {
            "matcher": {"id": "byRegexp", "options": "/üî• Errors.*/"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}
            ]
          },
          {
            "matcher": {"id": "byRegexp", "options": "/üíÄ DLQ.*/"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "dark-red"}}
            ]
          }
        ]
      },
      "alert": {
        "conditions": [
          {
            "evaluator": {"params": [0.1], "type": "gt"},
            "operator": {"type": "and"},
            "query": {"model": "A", "params": ["5m", "now"]},
            "reducer": {"params": [], "type": "avg"},
            "type": "query"
          }
        ],
        "executionErrorState": "alerting",
        "for": "2m",
        "frequency": "10s",
        "handler": 1,
        "name": "High Error Rate Alert",
        "noDataState": "no_data",
        "notifications": []
      },
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 18}
    },
    {
      "id": 4,
      "title": "‚è±Ô∏è Performance Metrics - Latency Distribution",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(consumer_task_processing_duration_seconds_bucket{task_type=~\"$task_type\"}[5m]))",
          "legendFormat": "üìä P50: {{task_type}}",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(consumer_task_processing_duration_seconds_bucket{task_type=~\"$task_type\"}[5m]))",
          "legendFormat": "üìà P95: {{task_type}}",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, rate(consumer_task_processing_duration_seconds_bucket{task_type=~\"$task_type\"}[5m]))",
          "legendFormat": "üî¥ P99: {{task_type}}",
          "refId": "C"
        },
        {
          "expr": "consumer_queue_wait_time_seconds{queue_name=~\"$queue_name\"}",
          "legendFormat": "‚è≥ Queue Wait: {{queue_name}}",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "never",
            "stacking": {"mode": "none"}
          },
          "color": {"mode": "palette-classic"},
          "unit": "s",
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 5},
              {"color": "orange", "value": 10},
              {"color": "red", "value": 30}
            ]
          }
        },
        "overrides": [
          {
            "matcher": {"id": "byRegexp", "options": "/üî¥ P99.*/"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}},
              {"id": "custom.lineWidth", "value": 3}
            ]
          }
        ]
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 26}
    },
    {
      "id": 5,
      "title": "üîÑ Active Tasks & Throughput",
      "type": "timeseries",
      "targets": [
        {
          "expr": "consumer_active_tasks{task_type=~\"$task_type\"}",
          "legendFormat": "üü¢ Active: {{task_type}}",
          "refId": "A"
        },
        {
          "expr": "rate(consumer_tasks_processed_total{queue_name=~\"$queue_name\"}[1m]) * 60",
          "legendFormat": "‚ö° Throughput: {{queue_name}}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 20,
            "showPoints": "never",
            "stacking": {"mode": "normal"}
          },
          "color": {"mode": "palette-classic"},
          "unit": "short",
          "min": 0
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 26}
    },
    {
      "id": 6,
      "title": "üìà Success Rate by Task Type",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(consumer_tasks_processed_total{status=\"success\", task_type=~\"$task_type\"}[5m]) / (rate(consumer_tasks_processed_total{status=\"success\", task_type=~\"$task_type\"}[5m]) + rate(consumer_tasks_processed_total{status=\"failed\", task_type=~\"$task_type\"}[5m])) * 100",
          "legendFormat": "{{task_type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": null},
              {"color": "orange", "value": 80},
              {"color": "yellow", "value": 90},
              {"color": "green", "value": 95},
              {"color": "dark-green", "value": 99}
            ]
          },
          "unit": "percent",
          "decimals": 1,
          "min": 0,
          "max": 100
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "orientation": "horizontal"
      },
      "gridPos": {"h": 6, "w": 8, "x": 0, "y": 34}
    },
    {
      "id": 7,
      "title": "üîÅ Retry Statistics",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(increase(consumer_task_retries_total{task_type=~\"$task_type\"}[1h])) by (task_type)",
          "legendFormat": "{{task_type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 5},
              {"color": "orange", "value": 15},
              {"color": "red", "value": 30}
            ]
          },
          "unit": "short",
          "decimals": 0
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center"
      },
      "gridPos": {"h": 6, "w": 8, "x": 8, "y": 34}
    },
    {
      "id": 8,
      "title": "üíÄ Dead Letter Queue",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(increase(consumer_dead_letter_queue_total{task_type=~\"$task_type\"}[1h])) by (task_type, reason)",
          "legendFormat": "{{task_type}}: {{reason}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 1},
              {"color": "orange", "value": 5},
              {"color": "red", "value": 10}
            ]
          },
          "unit": "short",
          "decimals": 0,
          "noValue": "0"
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center"
      },
      "gridPos": {"h": 6, "w": 8, "x": 16, "y": 34}
    },
    {
      "id": 9,
      "title": "üé™ Task Distribution - Current Hour",
      "type": "piechart",
      "targets": [
        {
          "expr": "sum(increase(consumer_tasks_processed_total{status=\"success\", task_type=~\"$task_type\"}[1h])) by (task_type)",
          "legendFormat": "{{task_type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "short",
          "decimals": 0
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "pieType": "donut",
        "tooltip": {"mode": "single", "sort": "none"},
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": ["lastNotNull", "percent"],
          "values": ["value", "percent"]
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 40}
    },
    {
      "id": 10,
      "title": "‚öñÔ∏è Queue Load Distribution",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(consumer_tasks_processed_total{status=\"success\", queue_name=~\"$queue_name\"}[1m]) * 60",
          "legendFormat": "{{queue_name}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "fillOpacity": 80,
            "showPoints": "never",
            "stacking": {"mode": "normal"}
          },
          "color": {"mode": "palette-classic"},
          "unit": "reqps",
          "min": 0
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 40}
    },
    {
      "id": 11,
      "title": "üå°Ô∏è System Health Monitor",
      "type": "timeseries",
      "targets": [
        {
          "expr": "up{job=\"producer\"}",
          "legendFormat": "üü¢ Producer Health",
          "refId": "A"
        },
        {
          "expr": "up{job=\"consumer\"}",
          "legendFormat": "üü¢ Consumer Health",
          "refId": "B"
        },
        {
          "expr": "rate(producer_task_send_errors_total{task_type=~\"$task_type\"}[1m]) * 60",
          "legendFormat": "üî¥ Producer Errors: {{task_type}}",
          "refId": "C"
        },
        {
          "expr": "rate(http_request_duration_seconds_count{code!=\"200\"}[1m]) * 60",
          "legendFormat": "üü° HTTP Errors: {{code}}",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "stepAfter",
            "lineWidth": 2,
            "fillOpacity": 25,
            "showPoints": "always",
            "pointSize": 5
          },
          "color": {"mode": "palette-classic"},
          "unit": "short",
          "min": 0
        },
        "overrides": [
          {
            "matcher": {"id": "byRegexp", "options": "/üü¢.*/"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}},
              {"id": "custom.fillOpacity", "value": 50}
            ]
          },
          {
            "matcher": {"id": "byRegexp", "options": "/üî¥.*/"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}
            ]
          }
        ]
      },
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 48}
    },
    {
      "id": 12,
      "title": "üìã HTTP Performance & API Health",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
          "legendFormat": "üåê HTTP P95: {{endpoint}}",
          "refId": "A"
        },
        {
          "expr": "rate(http_request_duration_seconds_count[1m]) * 60",
          "legendFormat": "üìä Requests/min: {{endpoint}}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 15,
            "showPoints": "never"
          },
          "color": {"mode": "palette-classic"},
          "unit": "s",
          "min": 0
        }
      },
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 56}
    }
  ],
  "annotations": {
    "list": [
      {
        "name": "Task Queue Events",
        "enable": true,
        "iconColor": "rgba(255, 96, 96, 1)",
        "query": "ALERTS{alertname=~\".*Task.*|.*Consumer.*|.*Producer.*\"}",
        "titleFormat": "{{alertname}}",
        "textFormat": "{{summary}}: {{description}}",
        "tags": ["alerts", "task-queue"]
      },
      {
        "name": "High Error Rate",
        "enable": true,
        "iconColor": "rgba(255, 0, 0, 1)",
        "query": "increase(consumer_task_errors_total[5m]) > 5",
        "titleFormat": "Error Spike",
        "textFormat": "High error rate detected in {{task_type}}"
      }
    ]
  },
  "links": [
    {
      "icon": "external link",
      "tags": ["jaeger"],
      "title": "üîç View Traces in Jaeger",
