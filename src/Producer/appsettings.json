{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RabbitMQ": {
    "Host": "localhost",
    "Port": 5672,
    "Username": "admin",
    "Password": "admin123",
    "VirtualHost": "/",
    "Queues": {
      "ReportGeneration": "report-queue",
      "DataProcessing": "data-queue", 
      "EmailNotification": "email-queue",
      "FileProcessing": "file-queue",
      "DatabaseCleanup": "cleanup-queue",
      "Default": "task-queue"
    },
    "Exchange": {
      "Name": "task-exchange",
      "Type": "direct"
    },
    "DeadLetterQueue": {
      "Name": "dlq-queue",
      "Exchange": "dlq-exchange"
    }
  },
  "OpenTelemetry": {
    "ServiceName": "producer-app",
    "ServiceVersion": "1.0.0",
    "JaegerEndpoint": "http://localhost:14268/api/traces"
  },
  "Application": {
    "Port": 8080,
    "AutoSendTasks": true,
    "AutoSendInterval": 5000,
    "MaxRetryAttempts": 3
  },
  "AllowedHosts": "*"
} 