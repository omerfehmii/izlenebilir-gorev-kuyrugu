{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "AIService.Services.HybridAIService": "Information"
    }
  },
  "OpenTelemetry": {
    "ServiceName": "ai-service",
    "ServiceVersion": "2.0.0-hybrid-ai",
    "JaegerEndpoint": "http://jaeger:14268/api/traces"
  },
  "HybridAI": {
    "SyntheticDataSize": 8000,
    "EnableLearning": true,
    "ModelUpdateInterval": "24:00:00",
    "ConfidenceThreshold": 0.7,
    "MaxPredictionTime": 100
  },
  "AllowedHosts": "*"
}
